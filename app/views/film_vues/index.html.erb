<p id="notice"><%= notice %></p>

<h1>Listing Film Vues</h1>

<table>
  <thead>
    <tr>
      <th>Id utilisateur</th>
      <th>Id film</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @film_vues.each do |film_vue| %>
      <tr>
        <td><%= film_vue.id_utilisateur %></td>
        <td><%= film_vue.id_film %></td>
        <td><%= link_to 'Show', film_vue %></td>
        <td><%= link_to 'Edit', edit_film_vue_path(film_vue) %></td>
        <td><%= link_to 'Destroy', film_vue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Film vue', new_film_vue_path %>
